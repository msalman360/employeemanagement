<% if alert.present? %>
  <script>
      $(document).ready(function (){
          toastr.options.timeout = 1200;
          toastr.error('<%= alert %>');
      });
      <% flash[:alert] = nil %>
  </script>
<% end %>
<% if notice.present? %>
  <script>
      $(document).ready(function (){
          toastr.options.timeout = 1200;
          toastr.success('<%= notice %>');
      });
      <% flash[:notice] = nil %>
  </script>
<% end %>
